{
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "all",
  "useTabs": true,
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": [
        "*.json",
        "*.html"
      ],
      "options": {
        "useTabs": false,
        "tabWidth": 2
      }
    }
  ],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "react",
    "react-router-dom",
    "@mui",
    "app",
    "<THIRD_PARTY_MODULES>",
    "pages",
    "components",
    "reactkit",
    "models",
    "utils",
    "api",
    "actions",
    "context",
    "assets",
    "[./]",
    "[../]"
  ],
  "importOrderSeparation": true
}
