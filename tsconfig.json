{
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  },
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": "src",
    "rootDir": "src",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/telegram-webapps-types-new"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "*": [
        "./*"
      ],
      "actions/*": [
        "./actions/*"
      ],
      "api/*": [
        "./api/*"
      ],
      "app/*": [
        "./app/*"
      ],
      "assets/*": [
        "./assets/*"
      ],
      "common/*": [
        "./common/*"
      ],
      "pages/*": [
        "./pages/*"
      ],
      "ui/*": [
        "./ui/*"
      ],
    }
  },
  "include": [
    "src/**/*"
  ]
}
